- if current_dwelling
  = link_to current_dwelling.name, root_path, :class => 'brand'
- else
  = link_to "Roomie", root_path, :class => 'brand'
%ul.nav
  - if current_user
    %li
      = link_to(current_user.name, user_path(current_user))
    %li
      = link_to('Logout', logout_path)
  -else
    %li
      = link_to('Login', login_path)
    %li
      = link_to('Sign Up', signup_path)
